{
  "compilerOptions": {
   "target": "es2016",
   "lib": ["es6"],
   "module": "commonjs",
   "rootDir": "./src",
   "allowJs": true,
   "outDir": "./dist",
   "esModuleInterop": true,
   "forceConsistentCasingInFileNames": true,
   "strict": true,
   "noImplicitAny": true,
   "skipLibCheck": true,
   "resolveJsonModule": true,
   "experimentalDecorators": true,
   "emitDecoratorMetadata": true,
   }
}